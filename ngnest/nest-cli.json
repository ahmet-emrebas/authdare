{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "version": "1",
  "monorepo": true,
  "root": "api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "models": {
      "type": "library",
      "root": "libs/models",
      "entryFile": "index",
      "sourceRoot": "libs/models/src",
      "compilerOptions": {
        "tsConfigPath": "libs/models/tsconfig.lib.json"
      }
    },
    "decorators": {
      "type": "library",
      "root": "libs/decorators",
      "entryFile": "index",
      "sourceRoot": "libs/decorators/src",
      "compilerOptions": {
        "tsConfigPath": "libs/decorators/tsconfig.lib.json"
      }
    },
    "http": {
      "type": "library",
      "root": "libs/http",
      "entryFile": "index",
      "sourceRoot": "libs/http/src",
      "compilerOptions": {
        "tsConfigPath": "libs/http/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "resources": {
      "type": "library",
      "root": "libs/resources",
      "entryFile": "index",
      "sourceRoot": "libs/resources/src",
      "compilerOptions": {
        "tsConfigPath": "libs/resources/tsconfig.lib.json"
      }
    }
  }
}