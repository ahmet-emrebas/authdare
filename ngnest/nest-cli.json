{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "version": "1",
  "monorepo": true,
  "root": "api",
  "newProjectRoot": "libs/nest",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "server": {
      "type": "application",
      "root": "apps/server",
      "entryFile": "main",
      "sourceRoot": "apps/server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/server/tsconfig.app.json"
      }
    },
    "auth-user": {
      "type": "library",
      "root": "libs/auth-user",
      "entryFile": "index",
      "sourceRoot": "libs/auth-user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth-user/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "resource": {
      "type": "library",
      "root": "libs/resource",
      "entryFile": "index",
      "sourceRoot": "libs/resource/src",
      "compilerOptions": {
        "tsConfigPath": "libs/resource/tsconfig.lib.json"
      }
    }
  }
}