{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": true,
    "incremental": true,
    "downlevelIteration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2017",
    "paths": {
      "@authdare/sidenav": [
        "libs/angular/sidenav/src/lib"
      ],
      "@authdare/sidenav/*": [
        "libs/angular/sidenav/src/lib/*"
      ],
      "@authdare/navbar": [
        "libs/angular/navbar/src/lib"
      ],
      "@authdare/navbar/*": [
        "libs/angular/navbar/src/lib/*"
      ],
      "@authdare/table": [
        "libs/angular/table/src/lib"
      ],
      "@authdare/table/*": [
        "libs/angular/table/src/lib/*"
      ],
      "@authdare/carousel": [
        "libs/angular/carousel/src/lib"
      ],
      "@authdare/carousel/*": [
        "libs/angular/carousel/src/lib/*"
      ],
      "@authdare/form": [
        "libs/angular/form/src/lib"
      ],
      "@authdare/form/*": [
        "libs/angular/form/src/lib/*"
      ],
      "@authdare/chart": [
        "libs/angular/chart/src/lib"
      ],
      "@authdare/chart/*": [
        "libs/angular/chart/src/lib/*"
      ],
      "@authdare/typing": [
        "libs/angular/typing/src/lib"
      ],
      "@authdare/typing/*": [
        "libs/angular/typing/src/lib/*"
      ],
      "@authdare/common": [
        "libs/common/src"
      ],
      "@authdare/common/*": [
        "libs/common/src/*"
      ],
      "@authdare/utils": [
        "libs/utils/src"
      ],
      "@authdare/utils/*": [
        "libs/utils/src/*"
      ],
      "@authdare/core": [
        "libs/core/src"
      ],
      "@authdare/core/*": [
        "libs/core/src/*"
      ],
      "@authdare/config": [
        "libs/config"
      ],
      "@authdare/config/*": [
        "libs/config/*"
      ],
      "@authdare/models": [
        "libs/models"
      ],
      "@authdare/models/*": [
        "libs/models/*"
      ],
      "@authdare/modules": [
        "libs/modules"
      ],
      "@authdare/modules/*": [
        "libs/modules/*"
      ],
    },
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}