{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "module": "commonjs",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": true,
    "incremental": true,
    "downlevelIteration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2017",
    "paths": {
      "@authdare/auth": [
        "libs/auth/src"
      ],
      "@authdare/auth/*": [
        "libs/auth/src/*"
      ],
      "@authdare/utils": [
        "libs/utils/src"
      ],
      "@authdare/utils/*": [
        "libs/utils/src/*"
      ],
      "@authdare/objects": [
        "libs/objects/src"
      ],
      "@authdare/objects/*": [
        "libs/objects/src/*"
      ],
      "@authdare/resources": [
        "libs/resources/src"
      ],
      "@authdare/resources/*": [
        "libs/resources/src/*"
      ],
      "@authdare/interfaces": [
        "libs/interfaces/src"
      ],
      "@authdare/interfaces/*": [
        "libs/interfaces/src/*"
      ],
      "@authdare/decorators": [
        "libs/decorators/src"
      ],
      "@authdare/decorators/*": [
        "libs/decorators/src/*"
      ]
    },
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}