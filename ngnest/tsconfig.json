{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "module": "commonjs",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": true,
    "incremental": true,
    "downlevelIteration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2017",
    "paths": {
      "@authdare/common": [
        "libs/common/src"
      ],
      "@authdare/common/*": [
        "libs/common/src/*"
      ],
      "@authdare/models": [
        "libs/models/src"
      ],
      "@authdare/models/*": [
        "libs/models/src/*"
      ],
      "@authdare/config": [
        "libs/config/src"
      ],
      "@authdare/config/*": [
        "libs/config/src/*"
      ],
      "@authdare/a18n": [
        "libs/a18n/src"
      ],
      "@authdare/a18n/*": [
        "libs/a18n/src/*"
      ],
      "@authdare/i18n": [
        "libs/i18n/src"
      ],
      "@authdare/i18n/*": [
        "libs/i18n/src/*"
      ]
    },
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}