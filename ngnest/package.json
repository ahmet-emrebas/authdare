  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.(spec)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/api",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@authdare/auth/(.*)": "<rootDir>/libs/auth/src/$1",
      "@authdare/auth": "<rootDir>/libs/auth/src",
      "@authdare/utils/(.*)": "<rootDir>/libs/utils/src/$1",
      "@authdare/utils": "<rootDir>/libs/utils/src",
      "@authdare/objects/(.*)": "<rootDir>/libs/objects/src/$1",
      "@authdare/objects": "<rootDir>/libs/objects/src",
      "@authdare/resources/(.*)": "<rootDir>/libs/resources/src/$1",
      "@authdare/resources": "<rootDir>/libs/resources/src",
      "@authdare/interfaces/(.*)": "<rootDir>/libs/interfaces/src/$1",
      "@authdare/interfaces": "<rootDir>/libs/interfaces/src",
      "@authdare/decorators/(.*)": "<rootDir>/libs/decorators/src/$1",
      "@authdare/decorators": "<rootDir>/libs/decorators/src",
      "^@authdare/database(|/.*)$": "<rootDir>/libs/database/src/$1"
    }
  }
