    "mocha": "^9.0.3",
    "ng-packagr": "^12.1.0",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/api",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@authdare/models/(.*)": "<rootDir>/libs/models/src/$1",
      "@authdare/models": "<rootDir>/libs/models/src",
      "^@authdare/decorators(|/.*)$": "<rootDir>/libs/decorators/src/$1",
      "^@authdare/http(|/.*)$": "<rootDir>/libs/http/src/$1",
      "^@authdare/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^@authdare/resources(|/.*)$": "<rootDir>/libs/resources/src/$1"
    }
  }
}