{
  "name": "ngnest",
  "version": "0.0.1",
  "description": "Api",
  "author": "Ahmet Emrebas",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "client:start": "ng serve",
    "client:build": "ng build",
    "client:build:watch": "ng build --watch --configuration development",
    "client:test": "ng test",
    "i18n": "ng extract-i18n --output-path apps/client/src/locale --format=json",
    "doc": "compodoc -p tsconfig.doc.json -d ../docs && ng build --prod --base-href=/authdare/materials/ --deploy-url https://ahmet-emrebas.github.io/authdare/materials/",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "server:build": "nest build && cp package.server-prod.json dist/api/package.json && cp public/favicon.ico dist/api/favicon.ico",
    "server:deploy": "cd dist/api && git commit -am'update' && git push heroku master",
    "server:start": "nest start --watch",
    "server:start:debug": "nest start --debug --watch",
    "server:test": "jest",
    "server:test:watch": "jest --watch",
    "server:test:cov": "jest --coverage",
    "server:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "server:test:e2e": "jest --config ./apps/ngnest/test/jest-e2e.json"
  },
