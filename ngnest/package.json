  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "c:ng": "ng",
    "c:start": "ng serve",
    "c:build": "ng build",
    "c:watch": "ng build --watch --configuration development",
    "c:test": "ng test",
    "i18n": "ng extract-i18n --output-path apps/client/src/locale --format=json",
    "doc": "compodoc -p tsconfig.doc.json -d ../docs && ng build --base-href=/authdare/materials --deploy-url https://ahmet-emrebas.github.io/authdare/materials/",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "s:prebuild": "rimraf dist",
    "s:build": "nest build",
    "s:format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "s:start": "nest start",
    "s:start:dev": "nest start --watch",
    "s:start:debug": "nest start --debug --watch",
    "s:start:prod": "node dist/main",
    "s:test": "jest",
    "s:s:test": "jasmine apps/api/**/*.spec.ts",
    "s:test:watch": "jest --watch",
    "s:test:cov": "jest --coverage",
    "s:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "s:test:e2e": "jest --config ./apps/ngnest/test/jest-e2e.json"
  },
