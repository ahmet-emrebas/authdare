{
  "name": "blueprint",
  "description": "Blueprint",
  "author": "Ahmet Emrebas",
  "private": true,
  "scripts": {
    "client:start": "ng serve",
    "client:build": "ng build",
    "client:watch": "ng build --watch --configuration development",
    "client:test": "ng test",
    "doc": "compodoc -p tsconfig.doc.json -d ../docs/blueprint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "server:start": "nest start --watch",
    "server:start:debug": "nest start --debug --watch",
    "start": "node main.js env=prod",
    "server:test:watch": "jest --watch",
    "server:test:cov": "jest --coverage",
    "server:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "postserver:build": "cp -r mail dist/api && cp -r package.json dist/api",
    "server:build": "nest build",
    "test:e2e": "jest --verbose -i --watch  -c ./apps/api/test/jest-e2e.json -t Auth"
  },
