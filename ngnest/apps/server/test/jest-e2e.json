{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "verbose": true,
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@filters": "<rootDir>/../src/filters",
    "@filters/*": "<rootDir>/../src/filters/$1",
    "@interceptors": "<rootDir>/../src/interceptors",
    "@interceptors/*": "<rootDir>/../src/interceptors/$1",
    "@middlewares": "<rootDir>/../src/middlewares",
    "@middlewares/*": "<rootDir>/../src/middlewares/$1",
    "@pipes": "<rootDir>/../src/pipes",
    "@pipes/*": "<rootDir>/../src/pipes/$1",
    "@auth": "<rootDir>/../src/auth",
    "@auth/*": "<rootDir>/../src/auth/$1",
    "@base": "<rootDir>/../src/base",
    "@base/*": "<rootDir>/../src/base/$1",
    "@resources": "<rootDir>/../src/resources",
    "@resources/*": "<rootDir>/../src/resources/$1"
  }
}
