{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/authdare-server/src",
  "monorepo": true,
  "root": "apps/authdare-server",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/authdare-server/tsconfig.app.json"
  },
  "projects": {
    "authdare-server": {
      "type": "application",
      "root": "apps/authdare-server",
      "entryFile": "main",
      "sourceRoot": "apps/authdare-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authdare-server/tsconfig.app.json"
      }
    },
    "base": {
      "type": "library",
      "root": "libs/base",
      "entryFile": "index",
      "sourceRoot": "libs/base/src",
      "compilerOptions": {
        "tsConfigPath": "libs/base/tsconfig.lib.json"
      }
    },
    "resource": {
      "type": "library",
      "root": "libs/resource",
      "entryFile": "index",
      "sourceRoot": "libs/resource/src",
      "compilerOptions": {
        "tsConfigPath": "libs/resource/tsconfig.lib.json"
      }
    }
  }
}